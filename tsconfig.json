{
    "compilerOptions": {
        "noEmit": true,
        "checkJs": true,
        "module": "Node16",
        "strict": true,
        "noImplicitAny": true,
        "noImplicitReturns": true,
        "noImplicitThis": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "noFallthroughCasesInSwitch": true,
        "esModuleInterop": true
    }
}